{
  "name": "special-characters",
  "version": "1.0.0",
  "description": "A curated collection of special characters",
  "scripts": {
    "dev": "eleventy --serve & npx tailwindcss -i ./src/assets/styles.css -o ./_site/assets/styles.css --watch",
